{
	"name": "vscode-resx-editor",
	"displayName": "ResX Editor",
	"description": "ResX Editor",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.86.0"
	},
	"publisher": "8LWXpg",
	"categories": [
		"Other"
	],
	"keywords": [
		"resx",
		"resw"
	],
	"bugs": {
		"url": ""
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": ""
	},
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "resx.editor",
				"displayName": "ResX",
				"selector": [
					{
						"filenamePattern": "*.resx"
					},
					{
						"filenamePattern": "*.resw"
					}
				],
				"priority": "default"
			}
		],
		"languages": [
			{
				"id": "xml",
				"extensions": [
					".resx",
					".resw"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "rm -r ./out && npm run package",
		"package": "$env:NODE_ENV = 'production' && node ./esbuild.js",
		"compile": "rm -r ./out && node ./esbuild.js",
		"watch": "tsc -watch -p ./",
		"test": "vscode-test",
		"check-update": "npx npm-check-updates"
	},
	"devDependencies": {
		"@types/glob": "^8.1.0",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.11.20",
		"@types/vscode": "^1.86.0",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"@vscode/test-cli": "^0.0.6",
		"@vscode/test-electron": "^2.3.9",
		"esbuild": "^0.20.1",
		"eslint": "^8.57.0",
		"typescript": "^5.3.3"
	},
	"dependencies": {
		"fast-xml-parser": "^4.3.5"
	}
}
