{
	"name": "code-resx",
	"displayName": "ResX Viewer/Editor",
	"description": "Edit ResX and ResW files in Webview",
	"version": "0.2.0",
	"engines": {
		"vscode": "^1.86.0"
	},
	"publisher": "8LWXpg",
	"author": {
		"name": "8LWXpg"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"resx",
		"resw"
	],
	"bugs": {
		"url": "https://github.com/8LWXpg/vscode-resx/issues"
	},
	"homepage": "https://github.com/8LWXpg/vscode-resx",
	"repository": {
		"type": "git",
		"url": "https://github.com/8LWXpg/vscode-resx"
	},
	"license": "MIT",
	"icon": "assets/icon.png",
	"main": "./out/extension.js",
	"browser": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "resx.editor",
				"displayName": "ResX",
				"selector": [
					{
						"filenamePattern": "*.resx"
					},
					{
						"filenamePattern": "*.resw"
					}
				],
				"priority": "default"
			}
		],
		"languages": [
			{
				"id": "xml",
				"extensions": [
					".resx",
					".resw"
				]
			}
		],
		"commands": [
			{
				"command": "code-resx.createEmptyFile",
				"title": "ResX: Create Empty File"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "bun run compile",
		"compile": "rm -r ./out && bun ./esbuild.js",
		"watch": "tsc -watch -p ./",
		"check-update": "bunx npm-check-updates"
	},
	"devDependencies": {
		"@types/glob": "^8.1.0",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.12.13",
		"@types/vscode": "^1.86.0",
		"@typescript-eslint/eslint-plugin": "^7.11.0",
		"@typescript-eslint/parser": "^7.11.0",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.4.0",
		"esbuild": "^0.21.4",
		"eslint": "^9.3.0",
		"fast-xml-parser": "^4.4.0",
		"typescript": "^5.4.5"
	}
}
